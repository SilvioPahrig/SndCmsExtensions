                {% block cms_block_snd_big_five_grid_big_inner %}
                    {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                {% endblock %}
            </div>
        {% endblock %}


        <div class="col-md-7 snd_bfg_4icon_block" data-cms-element-id="{{ id }}">
